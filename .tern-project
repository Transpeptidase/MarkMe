{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "jquery",
    "underscore"
  ],
  "loadEagerly": [
    "src/**/*.js",
    "*.js",
    "lib/*.js"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {}
  }
}